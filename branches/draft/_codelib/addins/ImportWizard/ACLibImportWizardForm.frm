   If Len(FileString) > 0 Then
      Do While Left$(FileString, 1) = "\"
         FileString = Trim$(Mid$(FileString, 2))
         If Len(FileString) = 0 Then Exit Sub
      Loop
      AddFile CurrentLocalRepositoryPath & FileString
   End If

