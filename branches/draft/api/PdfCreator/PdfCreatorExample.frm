Option Compare Database
Option Explicit

Private Sub Form_Load()
    Me.tbFilePath = CodeProject.Path & "\"
    Me.tbFileName = "Example"
    Me.cboFileExt = ".pdf"
    Me.tbReportName = "TestReport"
    Me.checkboxOpenFile = True
End Sub

Private Sub btnCreatePdf_Click()
    
    
    
Const PROC_NAME As String = "btnCreatePdf_Click"
On Error GoTo ERROR_HANDLER
    
    Dim Result As Boolean
        Result = LibPdfCreatorTools.ConvertReportToPDF(Me.tbReportName, _
                 Me.tbFilePath & Me.tbFileName & Me.cboFileExt, _
                 Me.checkboxOpenFile, vbNullString, _
                 True, , , , False)
    

EXIT_HANDLER:
    On Error Resume Next
    Exit Sub

ERROR_HANDLER:
    MsgBox "Error " & Err.Number & " (" & Err.Description & ") in procedure " & PROC_NAME & " of VBA Dokument Form_PdfCreatorExample"
    Resume EXIT_HANDLER

End Sub